{
  "sourceMaps": "inline",
  "plugins": [
    "transform-class-properties",
    "transform-flow-strip-types",
    "transform-react-jsx"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "production": {
      "presets": ["es2015"],
      "plugins": ["transform-runtime"]
    }
  }
}
